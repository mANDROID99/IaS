<level>
  <groups>
    <group p="0,0,0">
      <submeshes>
        <sub m="block" p="0,0,0" s="6,6,6"/>
      </submeshes>

      <splits>
        <split axis="y" val="3" pivot="3, 3, 3" id="split_y"/>
        <split axis="x" val="3" pivot="3, 3, 3" id="split_x"/>
        <split axis="z" val="3" pivot="3, 3, 3" id="split_z"/>
      </splits>

      <tracks>
        <track startdir="up" down="forward" id="Track_1">
          <node p="3,1,-1"/>
          <node p="3,6,-1"/>
          <node p="3,6,2"/>
        </track>

        <track startdir="forward" down="forward" id="Track_2">
          <node p="3,6,2"/>
          <node p="5,6,2"/>
          <node p="5,6,4"/>
          <node p="2,6,4"/>
        </track>

        <track startdir="forward" down="forward" id="Track_3">
          <node p="3,6,2"/>
          <node p="0,6,2"/>
          <node p="0,6,4"/>
          <node p="3,6,4"/>
        </track>

        <track startdir="down" down="forward" id="Track_4">
          <node p="3,0,-1"/>
          <node p="5,0,-1"/>
          <node p="5,-2,-1"/>
          <node p="2,-2,-1"/>
        </track>
        
        <track startdir="down" down="forward" id="Track_5">
        <node p="3,0,-1"/>
        <node p="0,0,-1"/>
        <node p="0,-2,-1"/>
        <node p="3,-2,-1"/>
        </track>
      </tracks>

      <junctions>
        <junction branch_default="@Track_2" branch_alternate="@Track_3" type="OneToMany"/>
        <junction branch_default="@Track_4" branch_alternate="@Track_5" type="OneToMany"/>
      </junctions>
      
    </group>
  </groups>
</level>
